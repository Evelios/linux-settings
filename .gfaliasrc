function smallfsif {
  grep -e "^ [0-9]" $1 > ${1}_small
}

function formatsmall {
  for sif in "$@"; do
    formatsif $sif
    fsif=$(echo $sif | sed -e "s/\.sifpat/\.fsifpat/")
    echo "Creating small sfipat ${fsif}_small"
    smallfsif $fsif
  done
}

function fl {
  last=$(ls -tr *.sifpat | tail -1)
  formatsmall $last
}

