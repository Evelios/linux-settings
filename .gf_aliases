# ---- Command Shortcuts -------------------------------------------------------

alias diffsb="diff -r --exclude='program'"

function build {
  make_recipe=${1:-all}
  echo "Building Recipe $make_recipe"
  make $make_recipe > build.log 2> build.err
  grep Made build.log
  echo ""
  echo "---- Listing Build Errors ----"
  grep -i error build.err
}

function lnsrc {
  rm src
  ln -s $1 src
}

